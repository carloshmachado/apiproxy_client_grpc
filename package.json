{
  "name": "model-nfe",
  "version": "0.0.1",
  "description": "apiproxy",
  "main": "index.js",
  "scripts": {
    "generate": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "build": "npx tsc --build",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/proto/ --grpc_out=grpc_js:./src/proto/ meuprotobuf.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/proto/ meuprotobuf.proto"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.0",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.11",
    "@prisma/client": "^4.4.0",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "axios": "^1.1.3",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "https": "^1.0.0",
    "joi": "^17.7.0",
    "olimpo-utils": "^2.2.13"
  }
}